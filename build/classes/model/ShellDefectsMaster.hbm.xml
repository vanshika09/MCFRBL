<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="beans.ShellDefectsMaster" table="shell_defects_master">
		<id name="shellDefectId" type="java.lang.Integer">
			<column name="shell_defect_id"></column>
			<generator class="increment"></generator>
		</id>
		<property name="shellDefectDescription" type="java.lang.String">
			<column name="shell_defect_description" length="150"></column>
		</property>
				
	</class>
</hibernate-mapping>