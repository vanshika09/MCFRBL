<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="model.Abnormality" table="abnormality" >
		<id name="abid" type="java.lang.Integer">
			<column name="abid"></column>
			<generator class="increment"></generator>
		</id>
		<property name="abnormalityDescription" type="java.lang.String">
			<column name="abnormalitydescription" length="150"></column>
		</property>
		<property name="detailAbnormalityDescription" type="java.lang.String">
			<column name="detail_abnormalitydescription" length="500"></column>
		</property>		
		<property name="createdate" type="timestamp">
			<column name="createdate" length="30"></column>
		</property>
		<property name="shop" type="java.lang.String">
			<column name="shop" length="25"></column>
		</property>
		<property name="entrydate" type="java.lang.String">
			<column name="entry_date" length="30"></column>
		</property>
		<property name="entryBy" type="java.lang.String">
			<column name="entry_by" length="15"></column>
		</property>
		<property name="abnormalityID" type="java.lang.Integer">
			<column name="abnormality_id" ></column>
		</property>
		<property name="actionPerformed" type="java.lang.String">
			<column name="action_performed" ></column>
		</property>
		<property name="remarks" type="java.lang.String">
			<column name="remarks" ></column>
		</property>
		<property name="remarkEntrydate" type="java.lang.String">
			<column name="remark_entry_date" length="30"></column>
		</property>
		<property name="remarkEntryBy" type="java.lang.String">
			<column name="remark_entry_by" length="15"></column>
		</property>
		<property name="status" type="java.lang.String">
			<column name="status" length="25"></column>
		</property>
		
	</class>
</hibernate-mapping>