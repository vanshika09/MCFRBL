<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="beans.ShiftRoasterTransaction" table="shift_roaster_transaction" >
		<id name="empId" type="java.lang.String">
			<column name="emp_id" length="10"></column>
			
		</id>
		<property name="shiftId" type="java.lang.String">
			<column name="shift_id" length="15"></column>
		</property>
		<property name="validityFrom" type="date">
			<column name="validity_from" ></column>
		</property>		
		<property name="entryDate" type="java.lang.String">
			<column name="entry_date" length="20"></column>
		</property>
		<property name="entryBy" type="java.lang.String">
			<column name="entry_by" length="15"></column>
		</property>
		
	</class>
</hibernate-mapping>