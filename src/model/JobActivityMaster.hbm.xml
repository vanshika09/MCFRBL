<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="beans.JobActivityMaster" table="job_activity_master" >
		<id name="jobActivityId" type="java.lang.String">
			<column name="job_activity_id" length="20"></column>	
		</id>
		<property name="subsectionId" type="java.lang.String">
			<column name="subsection_id" length="20"></column>
		</property>
		<property name="jobActivityDescription" type="java.lang.String">
			<column name="job_activity_description" length="50"></column>
		</property>		
		
		
	</class>
</hibernate-mapping>