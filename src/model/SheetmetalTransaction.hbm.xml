<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">


<hibernate-mapping>
	<class name="beans.SheetmetalTransaction" table="sheetmetal_transaction" >
		
		<composite-id>
		<key-property name="drawingNo" type="java.lang.String">
			<column name="drawing_no" length="20"></column>
			
		</key-property>
		<key-property name="productionDate" type="date">
			<column name="production_date" ></column>
		</key-property>	
		</composite-id>
		<property name="qtyProduced" type="java.lang.Integer">
			<column name="qty_produced" length="15"></column>
		</property>
			
		
	</class>
</hibernate-mapping>