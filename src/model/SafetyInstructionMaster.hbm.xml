<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="beans.SafetyInstructionMaster" table="safety_instruction_master" >
	
	    
	  <id name="safety_instruction_id" type="java.lang.Integer">
			<column name="safety_instruction_id"></column>
			<generator class="increment"></generator>
		</id>
		<property name="safety_instruction_description" type="java.lang.String" >
		<column name="safety_instruction_description" ></column>
		</property>
		
	
		
		
		
	</class>
</hibernate-mapping>